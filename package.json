{
  "name": "vscode-wizard",
  "author": "redhat",
  "publisher": "redhat",
  "version": "0.0.2",
  "description": "A mid-weight page framework for generating wizards in a vscode webview.",
  "license": "Apache-2.0",
  "tags": [
    "functions",
    "classes",
    "typescript",
    "tools",
    "helpers",
    "vscode"
  ],
  "keywords": [
    "functions",
    "classes",
    "js",
    "ecmascript",
    "javascript",
    "typescript",
    "tools",
    "helpers",
    "vscode",
    "visual",
    "studio",
    "code",
    "extensions",
    "modules"
  ],
  "engines": {
    "vscode": "^1.42.0"
  },
  "main": "lib/wizardActivator.js",
  "typings": "lib/wizardActivator.d.ts",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "ext.home",
        "title": "Show Example Page"
      }
    ]
  },
  "scripts": {
    "build": "npm run clean && eslint . --ext .ts && npm run compile",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepare": "rm -rf lib && tsc && tsc -d",
    "prepublishOnly": "npm run compile && npm run lint",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "mocha out/test/**/*.test.js",
    "check": "gts check",
    "clean": "gts clean",
    "fix": "gts fix",
    "posttest": "npm run check"
  },
  "devDependencies": {
    "@types/mocha": "^7.0.1",
    "@types/vscode": "^1.42.0",
    "@types/node": "^12.11.7",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "handlebars": "^4.7.3",
    "gts": "^1.1.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/robstryker/vscode-wizard.git"
  },
  "bugs": {
    "url": "https://github.com/robstryker/vscode-wizard/issues"
  },
  "homepage": "https://github.com/robstryker/vscode-wizard/blob/master/README.md"
}
